{
  "name": "musicplayer",
  "version": "1.0.0",
  "description": "html5音乐播放器",
  "main": "index.js",
  "scripts": {
    "server": "webpack-dev-server --inline --hot --host 127.0.0.1 --port 8081",
    "description": "上面webpack端口冲突,所以自定义端口,可自行修改.",
    "start": "webpack-dev-server --inline --hot",
    "build": "webpack --display-error-details",
    "watch": "webpack --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@git.umlife.net:godzilla/musicPlayer.git"
  },
  "keywords": [
    "html5",
    "audio"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "compass-mixins": "^0.12.10",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.10.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true,
      "spread": true,
      "restParams": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "cors": "^2.7.1",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "webpack-dev-server": "^1.14.1"
  }
